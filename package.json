{
  "name": "cogito",
  "version": "1.0.0",
  "description": "Multi-personality AI coordination system with database-driven personality evolution",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev-all": "npm start",
    "setup-postgres": "node scripts/setup-postgres.js",
    "apply-schema": "node scripts/apply-schema.js",
    "migrate-from-sqlite": "node scripts/migrate-from-sqlite.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs postgres",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "lint": "eslint database/ ai-agents/ services/ uploads/ classification/ integrations/ server/ scripts/ tests/",
    "lint:fix": "eslint database/ ai-agents/ services/ uploads/ classification/ integrations/ server/ scripts/ tests/ --fix",
    "lint:check-sizes": "eslint database/ ai-agents/ services/ uploads/ classification/ integrations/ server/ --rule '{\"max-lines\": [\"error\", {\"max\": 200}]}'",
    "check-size": "node scripts/check-file-size.js",
    "pre-write-check": "node scripts/pre-write-check.js",
    "mcp-server": "node mcp-game-state-server.js",
    "prod-db": "node scripts/run-on-prod.js"
  },
  "keywords": [
    "ai",
    "personality-evolution",
    "consciousness",
    "multi-agent",
    "conversation-intelligence"
  ],
  "author": "Claude (Et al.) <claude@anthropic.com>",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "file-type": "^21.0.0",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "openai": "^5.6.0",
    "pdf-lib": "^1.17.1",
    "pdf2json": "^3.2.0",
    "pg": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "imports": {
    "#database/*": "./database/*",
    "#ai-agents/*": "./ai-agents/*",
    "#services/*": "./services/*",
    "#uploads/*": "./uploads/*",
    "#classification/*": "./classification/*",
    "#integrations/*": "./integrations/*",
    "#server/api/*": "./server/api/*",
    "#server/auth/*": "./server/auth/*",
    "#server/conversations/*": "./server/conversations/*",
    "#server/uploads/*": "./server/uploads/*",
    "#server/meetings/*": "./server/meetings/*",
    "#server/clients/*": "./server/clients/*",
    "#server/events/*": "./server/events/*",
    "#server/summaries/*": "./server/summaries/*",
    "#server/*": "./server/*"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {}
  }
}
